{
  "rewrites": [
    {
      "source": "/game/Christmas25",
      "destination": "/index.html"
    },
    {
      "source": "/game/Christmas25/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/game/christmas25",
      "destination": "/index.html"
    },
    {
      "source": "/game/christmas25/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.crackwits.com"
        }
      ],
      "destination": "https://crackwits.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/game/Christmas25/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

